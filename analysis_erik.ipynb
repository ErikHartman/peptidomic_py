    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import os\n",
    "import xlrd\n",
    "import regex as re\n",
    "\n",
    "path = os.getcwd()\n",
    "files = os.listdir(path)\n",
    "# gets all xlsx files in cwd\n",
    "files_xlsx = [f for f in files if f[-4:] == 'xlsx']\n",
    "df = pd.DataFrame()\n",
    "for file in files_xlsx:\n",
    "    data = pd.read_excel(file, 'peptide')\n",
    "    df = df.append(data)  # appends all data\n",
    "    print(file,\" has \", (len(df.index) ,  \" peptides\"))\n",
    "\n",
    "number_of_total_peptides=len(df.index)\n",
    "df = df.loc[(df['#Feature'] >= 4)] # sorts out if feature<4\n",
    "df['count'] = 1\n",
    "df = df.groupby(['Accession','Peptide']).sum()\n",
    "df.reset_index(['Peptide'], inplace=True)\n",
    "df['N-cut']=df['Peptide'].apply(lambda x: x[0:4])\n",
    "df['C-cut']=df['Peptide'].apply(lambda x: x[len(x):len(x)-5:-1])\n",
